<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:MileageTrackerMobile.ViewModels"
             x:DataType="viewModels:MainViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MileageTrackerMobile.Modules.LogsList">
    <StackPanel DockPanel.Dock="Top"
                VerticalAlignment="Stretch"
                HorizontalAlignment="Stretch"
                IsVisible="{Binding LogsPageVisible}">
                            
        <TextBlock Margin="20 0 0 25" Classes="bold" FontSize="20">Your Logs:</TextBlock>
                            
        <ListBox x:Name="LogsListitem" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="20 0 20 0" Height="400">
            <ListBox.Template>
                
            </ListBox.Template>
        </ListBox>
                                
                            
    </StackPanel>
</UserControl>
